body
  background-color: #3498DB
  color: #34495e
  font: 14px/1.231 "Lato", sans-serif

.jumbotron
  background-color: #34495E

.jumbotron .container
  height: 250px
  background-image: url(/images/jumbotron-background.gif)
  text-align: center

.jumbotron h1
  color: #ffffff
  background-color: rgba(0, 0, 0, 0.5)
  width: 400px
  height: 70px
  margin: 100px auto 0 auto
  font-size: 40px
  border-radius: 10px
  line-height: 70px

.contents
  margin-bottom: 30px

.languagebox
  text-align: center
  margin-top: 40px
  margin-bottom: 30px
  height: 100px

.language
  display: inline-block
  width: 100px

.language a
  text-decoration: none
  font-size: 80px
  color: #34495E

.language a:hover
  color: #ffffff

.container section
  color: #ffffff
  padding: 10px

.container section
  padding-left: 31px

.container section h2
  boder-bottom: 1px solid #34668d
  margin-left: -31px

.graph .chart
  text-align: center

.graph .desc


.example .fui-bubble-16
  font-size: 14px
  margin-right: 10px

.example pre
  margin-left: 25px
  margin-bottom: 10px

footer
  background-color: #F1C40F
  color: #ffffff
  padding: 20px
  text-align: center

// disable bootstrap code syntax highlight
pre
  margin: 0
  border-radius: 0
  border: none
  background-color: transparent
  line-height: 14px
  padding: 0
  font-size: 12px

